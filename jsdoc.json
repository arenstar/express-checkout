{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "src",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "search": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "search": true
    }
}
